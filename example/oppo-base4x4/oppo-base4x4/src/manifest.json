{
  "package": "com.oppo.card",
  "name": "oppo卡片示例",
  "versionName": "1.0.0",
  "versionCode": 1,
  "minPlatformVersion": 1070,
  "icon": "",
  "features": [
    {
      "name": "system.configuration"
    },
    {
      "name": "system.network"
    },
    {
      "name": "system.geolocation"
    },
    {
      "name": "system.device"
    }
